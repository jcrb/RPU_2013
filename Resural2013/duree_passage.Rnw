\section{Selon l'orientation}
<<duree_orientation,echo=FALSE,results='asis'>>=
#
# pour tout le groupe
#
# durée d'attente moyenne selon l'orientation

t <- round(tapply(d1$presence,d1$ORIENTATION,mean,na.rm=TRUE),2)
t <- sort(t)
